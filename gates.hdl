import (
    "builtin"
)

chip And(a wire, b wire) wire {
    tmp := builtin.Nand(a, b)
    res := builtin.Nand(tmp, tmp)
    return res 
}

chip AndThreeWay(a wire, b wire, c wire) wire {
	tmp := And(a, b)
	res := And(tmp, c)
	return res
}

chip Not(a wire) wire {
	res := builtin.Nand(a, a)
	return res
}